output "external_id" {
  value       = local.iam_role_external_id
  description = "The External ID configured into the IAM role"
}

output "iam_role_name" {
  value       = local.iam_role_name
  description = "The IAM Role name"
}

output "iam_role_arn" {
  value       = local.iam_role_arn
  description = "The IAM Role ARN"
}

output "lacework_integration_guid" {
  description = "The GUID for the created Lacework integration"
  value       = lacework_integration_aws_cfg.default.intg_guid
}
